<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="AcpSF" prefHeight="336.0" prefWidth="472.0" style="-fx-background-color: #E0EEEE;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="ButattachfileSF" layoutX="298.0" layoutY="104.0" mnemonicParsing="false" onAction="#AttachFileSF" text="attach file" />
      <Button fx:id="ButcloseSF" layoutX="383.0" layoutY="287.0" mnemonicParsing="false" onAction="#closeSF" text="Close" />
      <Text fx:id="TxtfSF" layoutX="52.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select a file from your computer" />
      <TextField fx:id="TxtboxfilenameSF" layoutX="105.0" layoutY="104.0" onAction="#GetFileNameSF" promptText="File name" />
   </children>
</AnchorPane>
